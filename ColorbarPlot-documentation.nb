   RowBox[{"{", 
    RowBox[{
     RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"RegionFunction", "\[Rule]", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"1", "<", 
        RowBox[{"region", "[", 
         RowBox[{"#1", ",", "#2"}], "]"}]}], "&"}], "<", "6"}], ")"}]}]}], 
  "]"}]}], "Input"],

